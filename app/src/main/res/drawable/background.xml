<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <rotate
            android:fromDegrees="-35"
            android:toDegrees="0"
            android:pivotX="20%"
            android:pivotY="320%" >
            <shape
                android:shape="line"
                android:top="1dip" >
                <stroke
                    android:width="36dip"
                    android:color="#ffab91" />
            </shape>
        </rotate>
    </item>

</layer-list>